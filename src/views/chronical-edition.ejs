<%- include('./partials/head') %>

<%- include('./partials/header') %>
<!-- This is left sidebar  -->
<%- include('./partials/left-sidebar') %>
<!-- inline css -->
<!-- Navbar section -->
<main class="main" id="mainContent">
    <div class="base-section coustom-section">
        <div class="main-content">
            <div class="accordion" id="researchChronical">
                <div class="accordion-item">
                    <h2 class="accordion-header" id="vice-chancellor">
                        <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#vice-chancellor"
                            aria-expanded="true" aria-controls="vice-chancellor">
                            From Vice Chancellor's Desk #1
                        </button>
                    </h2>
                    <div id="vice-chancellor-colapse" class="accordion-collapse collapse show" aria-labelledby="vice-chancellor"
                        data-bs-parent="#researchChronical">
                        <div class="accordion-body">
                            <strong>From Vice Chancellor's Desk</strong>
                            <div id="vc-editor">
                                <p>This is some sample content.</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="accordion-item">
                    <h2 class="accordion-header" id="research-heading">
                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                            data-bs-target="#research" aria-expanded="false" aria-controls="research">
                            Research #2
                        </button>
                    </h2>
                    <div id="research" class="accordion-collapse collapse" aria-labelledby="research-heading"
                        data-bs-parent="#researchChronical">
                        <div class="accordion-body">
                            <strong>From Vice Chancellor's Desk</strong>
                            <div id="research-editor">
                                <p>This is some sample content.</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="accordion-item">
                    <h2 class="accordion-header" id="maeeting-data-head">
                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                            data-bs-target="#meeting-stackholders" aria-expanded="false" aria-controls="meeting-stackholders">
                            Meeting Stackholders #3
                        </button>
                    </h2>
                    <div id="meeting-stackholders" class="accordion-collapse collapse" aria-labelledby="maeeting-data-head"
                        data-bs-parent="#researchChronical">
                        <div class="accordion-body">
                            <strong>Meeting Stackholders</strong>
                            <div id="meeting-stackholders-editor">
                                <p>This is some sample content.</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="accordion-item">
                    <h2 class="accordion-header" id="branding-data-head">
                      <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#branding" aria-expanded="false" aria-controls="branding">
                        Branding #4
                      </button>
                    </h2>
                    <div id="branding" class="accordion-collapse collapse" aria-labelledby="branding-data-head" data-bs-parent="#researchChronical">
                        <div class="accordion-body">
                            <strong> Branding </strong>
                            <div id="branding-editor">
                                <p>This is some sample content.</p>
                            </div>
                        </div>
                    </div>
                  </div>
            </div>
        </div>
    </div>
  </main>
<script src="https://cdn.ckeditor.com/ckeditor5/41.1.0/classic/ckeditor.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js" integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF" crossorigin="anonymous"></script>
<script>

    let idContainerArray = [
        "#vc-editor",
        "#research-editor",
        "#meeting-stackholders-editor",
        "#branding-editor"
    ];
    for(let i = 0; i <= idContainerArray.length - 1; i++){
        ClassicEditor
        .create( document.querySelector( idContainerArray[i] ) )
        .catch( error => {
            console.error( error );
        } );
    }
</script>
