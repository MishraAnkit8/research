                                    <th class="col-lg-1">Serial no</th>
                                    <th class="col-lg-1">Ranking Description</th>
                                    <th class="col-lg-1">Accreditation</th>
                                    <th class="col-lg-1">Convocation at NMIMS</th>
                                    <th class="col-lg-1">School/Campus Achievements</th>
                                    <th class="col-lg-1">Inaugural Program at NMIMS</th>
                                    <th class="col-lg-1">Events at NMIMS</th>
                                    <th class="col-lg-1">Action</th>
                                </thead>
                                <tbody>
                                    <% meetingData.forEach(function(meeting, index) { %>
                                        <tr scope="row" data-meeting-id="<%- meeting.id %>"
                                            data-ranking-documents="<%- meeting.ranking_documents %>"
                                            data-ranking-link="<%- meeting.ranking_link %>"
                                            data-accreditation-documents="<%- meeting.accreditation_documents %>"
                                            data-accreditation-link="<%- meeting.accreditation_link %>"
                                            data-achievements-documents="<%- meeting.achievements_documents %>"
                                            data-achievements-link="<%- meeting.achievements_link %>"
                                            data-convocation-documents="<%- meeting.convocation_documents %>"
                                            data-convocation-link="<%- meeting.convocation_link %>"
                                            data-inaugural-program-documents="<%- meeting.inaugural_program_documents %>"
                                            data-inaugural-program-link="<%- meeting.inaugural_program_link %>"
                                            data-events-documents="<%- meeting.events_documents %>"
                                            data-events-link="<%- meeting.events_link %>">
                                            <td><%- index+1 %></td>
                                            <td class="ranking col-lg-1"><%- meeting.ranking %></td>
                                            <td class="accreditation col-lg-1"><%- meeting.accreditation %></td>
                                            <td class="convocation col-lg-1"><%- meeting.convocation %></td>
                                            <td class="achievements col-lg-1"><%- meeting.school_campus_achievements %>
                                            </td>
                                            <td class="inaugural-program col-lg-1"><%- meeting.inaugural_program %></td>
                                            <td class="events col-lg-1"><%- meeting.events %></td>
                                            <td class="col-lg-1">
                                                <label class="view" data-meeting-id="<%- meeting.id %>"><i class="fa-solid fa-eye view text-danger"
                                                        data-meeting-id="<%- meeting.id %>"></i></label>
                                                <label class="edit" data-meeting-id="<%- meeting.id %>"><i class="fa fa-edit edit text-danger"
                                                        data-meeting-id="<%- meeting.id %>"></i></label>
                                                <label class="delete" data-meeting-id="<%- meeting.id %>"><i class="fa-solid fa-trash delete text-danger"
                                                        data-meeting-id="<%- meeting.id %>"></i></label>
                                            </td>

                                            <!-- <td colspan="3px" style="display: flex;justify-content: center;">
                                                <b title="View" class="view border-right-0" style="cursor: pointer;"
                                                    data-meeting-id="<%- meeting.id %>">
                                                    &#128065;
                                                </b>&ensp;&ensp;&ensp;
                                                <b title="Edit" style="cursor: pointer;" class="edit border-right-0"
                                                    data-meeting-id="<%- meeting.id %>">
                                                    &#9998;
                                                </b>&ensp;&ensp;&ensp;
                                                <b title="Delete" style="cursor: pointer;" class="delete border-right-0"
                                                    data-meeting-id="<%- meeting.id %>">
                                                    &#128465;
                                                </b>

                                            </td> -->
