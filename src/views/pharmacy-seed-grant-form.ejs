<%- include('./partials/head') %>
    <!-- inline css -->
    <style>
        .modal-title-head {
            position: relative;
            text-align: center;
            padding-left: 30%;
            font-size: 20px;
        }

        table {
            border-collapse: collapse;
            width: 100%;
        }

        th,
        td {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }

        th {
            background-color: #f2f2f2;
        }
    </style>
    <!-- Navbar section -->
    <%- include('./partials/header') %>
        <!-- This is left sidebar  -->
        <%- include('./partials/left-sidebar') %>
            <main class="main" id="main-content">
                <div class="base-section coustom-section">
                    <!--DASHBOARD CONTENT START-->
                    <div class="errorHtml"></div>
                    <div class="breadcrumbs-container">
                        <ul class="breadcrumb">
                            <li class="breadcrumb-link">
                                <a href="<%- BASE_URL %>dashboard-page"> Dashboard </a>
                            </li>
                            <li class="breadcrumb-link">
                                <a href="<%- BASE_URL %>research"> Research </a>
                            </li>
                            <li class="breadcrumb-link">Pharmacy Seed Grant</li>
                        </ul>
                    </div>
                    <div>
                        <div class="table-card-header text-uppercase d-flex align-items-center justify-content-between">
                            <div>
                                <h5 class="pl-2">Pharmacy Seed Grant Form List</h5>
                            </div>
                            <div>
                                <span>ADD</span>
                                <button id="consultancy-add-new-form" type="button" class="btn btn-primary add-modal"
                                    data-toggle="modal" data-target="#pharmacy-seed-grant-modal">
                                    <i class="fas fa-plus plus"></i></button>
                                </button>
                            </div>
                        </div>
                        <div class="card-body table-responsive">
                            <div class="d-flex justify-content-between">
                                <div>
                                </div>
                                <div>
                                    <!-- <div class="table-searchbar-container">
                            <button type="button" id="searchBtn"><i class="fas fa-search"></i></button>
                            <input type="search" id="searchKeyword" placeholder="Enter keywords"
                                class="table-searchbar">
                        </div> -->
                                </div>
                            </div>
                            <table class="table vertical-lines custom-table table-responsive research-pagination"
                                id="pharmacy-seed-grant-form-list">
                                <thead>
                                    <th class="col-lg-1">Serial no</th>
                                    <th class="col-lg-1">Summary Title </th>
                                    <th class="col-lg-1">Project <Title></Title>
                                    </th>
                                    <th class="col-lg-1 principal-investigator-name">Principal Investigator</th>
                                    <th class="col-lg-1 co-investigator">Co Investigator</th>
                                    <th class="col-lg-1 research-status">Project Status</th>
                                    <th class="col-lg-1">Action</th>
                                </thead>
                                <% pharmacyData.forEach(function(pharmacy,index) {%>
                                    <tr data-pharmacy-id="<%-pharmacy.pharmacy_seed_id %>"
                                        data-summary-project-title="<%- pharmacy.summary_title %>" data="<%-pharmacy %>"
                                        data-project-duration="<%-pharmacy.project_duration %>"
                                        data-total-cost="<%-pharmacy.total_cost %>"
                                        data-consumables-amount="<%-pharmacy.consumables_amount %>"
                                        data-analysis-amount="<%-pharmacy.analysis_amount %>"
                                        data-other-amount="<%-pharmacy.other_amount %>"
                                        data-scientific-importance="<%-pharmacy.scientific_importance %>"
                                        data-project-title="<%-pharmacy.project_title %>"
                                        data-project-objectives="<%-pharmacy.project_objectives %>"
                                        data-detailed-methodology="<%-pharmacy.detailed_methodology %>"
                                        data-time-lines="<%-pharmacy.time_lines %>"
                                        data-budget-consumable-amount="<%-pharmacy.budget_consumable_amount %>"
                                        data-consumables-justification="<%-pharmacy.consumables_justification %>"
                                        data-solvents-amount="<%-pharmacy.solvents_amount %>"
                                        data-solvents-justification="<%-pharmacy.solvents_justification %>"
                                        data-chemicals-amount="<%-pharmacy.chemicals_amount %>"
                                        data-chemicals-justification="<%-pharmacy.chemicals_justification %>"
                                        data-biomarkers-reference-amount="<%-pharmacy.biomarkers_reference_amount %>"
                                        data-biomarkers-reference-justifications="<%-pharmacy.biomarkers_reference_justifications %>"
                                        data-hplc-amount="<%-pharmacy.hplc_amount %>"
                                        data-hplc-justification="<%-pharmacy.hplc_justification %>"
                                        data-experimental-animals-amount="<%-pharmacy.experimental_animals_amount %>"
                                        data-experimental-animals-justification="<%-pharmacy.experimental_animals_justification %>"
                                        data-cell-lines-amount="<%-pharmacy.cell_lines_amount %>"
                                        data-cell-lines-justifications="<%-pharmacy.cell_lines_justifications %>"
                                        data-kits-analysis-amount="<%-pharmacy.kits_analysis_amount %>"
                                        data-kits-analysis-justifications="<%-pharmacy.kits_analysis_justifications %>"
                                        data-evaluation-analysis-amount="<%-pharmacy.evaluation_analysis_amount %>"
                                        data-evaluation-analysis-justification="<%-pharmacy.evaluation_analysis_justification %>"
                                        data-proposed-out-come="<%-pharmacy.proposed_out_come %>"
                                        data-previous-project-explaination="<%-pharmacy.previous_project_explaination %>"
                                        data-pharmacy-references="<%-pharmacy.pharmacy_references %>"
                                        data-project-background="<%-pharmacy.project_background %>"
                                        data-hypothesis="<%-pharmacy.hypothesis %>"
                                        data-investigator-name="<%-pharmacy.investigator_name %>"
                                        data-investigator-dsg="<%-pharmacy.investigator_dsg %>"
                                        data-investigator-email="<%-pharmacy.investigator_email %>"
                                        data-investigator-mobile="<%-pharmacy.investigator_mobile %>"
                                        data-investigator-dob="<%-pharmacy.investigator_dob %>"
                                        data-investigator-address="<%-pharmacy.investigator_address %>"
                                        data-principal-name="<%-pharmacy.principal_name %>"
                                        data-principal-dsg="<%-pharmacy.principal_dsg %>"
                                        data-principal-org="<%-pharmacy.principal_org %>"
                                        data-principal-email="<%-pharmacy.principal_email %>"
                                        data-principal-mobile="<%-pharmacy.principal_mobile %>"
                                        data-co-investigator-name="<%-pharmacy.co_investigator_name %>"
                                        data-co-investigator-dsg="<%-pharmacy.co_investigator_dsg %>"
                                        data-co-investigator-org="<%-pharmacy.co_investigator_org %>"
                                        data-co-investigator-email="<%-pharmacy.co_investigator_email %>"
                                        data-co-investigator-mobile="<%- pharmacy.co_investigator_mobile %>"
                                        data-research-status="<%- pharmacy.project_status %>">
                                        <td><%- index + 1 %></td>
                                        <td><%- pharmacy.summary_title %></td>
                                        <td><%- pharmacy.project_title %></td>
                                        <td><%- pharmacy.principal_name %></td>
                                        <td><%- pharmacy.co_investigator_name %></td>
                                        <td class="research-status"><%- pharmacy.project_status %></td>
                                        <td colspan="3px" style="display:flex;justify-content: center;">
                                            <b title="View" class="view border-right-0" style="cursor: pointer;"
                                                data-pharmacy-id="<%-pharmacy.pharmacy_seed_id %>">
                                                &#128065;
                                            </b>&ensp;&ensp;&ensp;
                                            <b title="Edit" class="edit border-right-0" style="cursor: pointer;"
                                                data-pharmacy-id="<%-pharmacy.pharmacy_seed_id %>">
                                                &#9998;
                                            </b>&ensp;&ensp;&ensp;
                                            <b title="Delete" class="delete border-right-0" style="cursor: pointer;"
                                                data-pharmacy-id="<%-pharmacy.pharmacy_seed_id %>">
                                                &#128465;
                                            </b>
                                        </td>
                                    </tr>
                                    <% }); %>

                            </table>
                            <!-- <div class="d-flex justify-content-between">
                    <div>
                        <label id="total-row-count">Total entries : </label>
                        <label class="d-none" id="row-count"></label>
                    </div>
                    <div> -->
                            <!-- <button id="prevBtn" class="btn btn-primary">Previous</button>
                        <button id="nextBtn" class="btn btn-primary">Next</button>
                        <p id="pagination" class="pagination-search"></p> -->
                        </div>
                    </div>
                    <!--Import Modal -->
                    <div class="modal fade" id="open-program-modal" tabindex="-1" aria-labelledby="open-program-modal"
                        aria-hidden="true">
                    </div>
                    <!-- EDIT PROGRAM AND UPDATE -->
                    <div class="modal fade" id="edit-program-modal" tabindex="-1" aria-labelledby="editModalLabel"
                        aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title-head" id="editModalLabel">Update Program</h5>
                                    <button type="button" class="btn-close" data-bs-dismiss="modal"
                                        aria-label="Close"></button>
                                </div>
                                <div id="errorHtml"></div>
                                <div class="modal-body">
                                    <div class="mb-3">
                                        <label for="exampleInputEmail1" class=" sub-title">Program Name</label>
                                        <input type="text" class="form-control" id="program_name" name="program_name">
                                        <input type="hidden" class="form-control" id="hidden_id" name="hidden_id">
                                    </div>
                                    <div class="mb-3">
                                        <label for="exampleInputPassword1" class=" sub-title">Program Type</label>
                                        <select class="form-control" name="program_type_lid" id="program_type_lid">
                                            <option value="1">Full Time</option>
                                            <option value="2">Part Time</option>
                                        </select>
                                    </div>

                                    <div class="mb-3">
                                        <label for="abbr" class=" sub-title">Program Abbr</label>
                                        <input type="text" class="form-control" id="abbr" name="abbr">
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary"
                                        data-bs-dismiss="modal">Close</button>
                                    <button type="button" class="btn btn-primary updateProgram">Update</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </main>

            <!-- pharmacy seed grant modal  -->
            <div class="modal fade " id="pharmacy-seed-grant-modal" tabindex="-1" role="dialog"
                aria-labelledby="pharmacy-seed-grant-modal" aria-hidden="true">
                <div class="modal-dialog modal-dialog-scrollable modal-lg modal-fullscreen p-5" role="document">
                    <div class="modal-content modal-content-data validate-input">
                        <div class="modal-header">
                            <h4 class="modal-title-head" id="journal-paper-title">Pharmacy Seed Grant</h4>
                            <div class="close" data-dismiss="modal" aria-label="Close">
                                <span type="button" aria-hidden="true"><i class="fas fa-times "></i></span>
                            </div>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <hr class="bg-dark">
                                <div class="form-group col-lg-12 col-md-6 col-sm-12 mb-4">
                                    <label for="" class="form-label fw-bold modal-title-head">SECTION I Summary
                                        Sheet</label>
                                </div>
                                <hr>
                                <!-- Title of the project -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="project-title" class="form-label fw-bold">Title of the project </label>
                                    <input type="text" name="project-title" id="summary-project-title"
                                        data-validate="isEmpty,isExist, isLength:2:255"
                                        data-err-msg="This field is required and min length > 1" class="form-control"
                                        required />
                                </div>
                                <!-- 2.	Main objectives of project  -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="project-objectives" class="form-label fw-bold"> Main objectives of
                                        project </label>
                                    <input type="text" name="summary-project-objectives" id="summary-project-objectives"
                                        data-validate="isEmpty,isExist, isLength:2:255"
                                        data-err-msg="Provide Valid Input" class="form-control" required />
                                </div>
                                <!-- 3.	Details of Principal Investigator  -->
                                <hr class="bg-dark">
                                <div class="form-group col-lg-12 col-md-6 col-sm-12 mb-4">
                                    <label for="" class="form-label fw-bold modal-title-head">Details of Principal
                                        Investigator ID </label>
                                </div>
                                <hr>
                                <!-- name -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="principal-investigator-id" class="form-label fw-bold"> i. Name </label>
                                    <input type="text" name="principal-investigator-id" id="principal-investigator-name"
                                        data-validate="isEmpty,isNotNumber,isExist,isNotSpecialChar"
                                        data-err-msg="Provide Valid Input" class="form-control" required />
                                </div>
                                <!-- designation -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="principal-investigator-name" class="form-label fw-bold">ii. Designation
                                    </label>
                                    <input type="text" name="principal-investigator-name"
                                        id="principal-investigator-dsg"
                                        data-validate="isEmpty,isNotNumber,isExist,isNotSpecialChar"
                                        data-err-msg="Provide Valid Input" class="form-control" required />
                                </div>
                                <!-- institution -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="principal-investigator-id" class="form-label fw-bold"> iii. Name of
                                        Institution </label>
                                    <input type="text" name="principal-investigator-name"
                                        id="principal-investigator-org"
                                        data-validate="isEmpty,isNotNumber,isExist,isNotSpecialChar"
                                        data-err-msg="Provide Valid Input" class="form-control" required />
                                </div>
                                <!-- mobile -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="principal-investigator-mob" class="form-label fw-bold"> iv. Mobile
                                    </label>
                                    <input type="number" name="principal-investigator-org"
                                        id="principal-investigator-mob" data-validate="isEmpty,isNumber"
                                        data-err-msg="Provide Valid Input" class="form-control" required />
                                </div>
                                <!-- email -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="principal-investigator-email" class="form-label fw-bold"> v. Email
                                    </label>
                                    <input type="text" name="principal-investigator-mob"
                                        id="principal-investigator-email" data-validate="isEmpty,isExist"
                                        data-err-msg="Provide Valid Input" class="form-control" required />
                                </div>


                                <!--4.	Details of Co- Investigator (If any) -->

                                <!-- 3.	Details of Co Investigator  -->
                                <hr class="bg-dark">
                                <div class="form-group col-lg-12 col-md-6 col-sm-12 mb-4">
                                    <label for="" class="form-label fw-bold modal-title-head">Details of CO Investigator
                                        ID </label>
                                </div>
                                <hr>
                                <!-- name -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="co-investigator-name" class="form-label fw-bold"> i. Name </label>
                                    <input type="text" name="co-investigator-id" id="co-investigator-name"
                                        data-validate="isEmpty,isNotNumber,isExist,isNotSpecialChar"
                                        data-err-msg="Provide Valid Input" class="form-control" required />
                                </div>
                                <!-- designation -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="co-investigator-dsg" class="form-label fw-bold">ii. Designation </label>
                                    <input type="text" name="co-investigator-dsg" id="co-investigator-dsg"
                                        data-validate="isEmpty,isNotNumber,isExist,isNotSpecialChar"
                                        data-err-msg="Provide Valid Input" class="form-control" required />
                                </div>
                                <!-- institution -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="co-investigator-org" class="form-label fw-bold"> iii. Name of
                                        Institution </label>
                                    <input type="text" name="co-investigator-name" id="co-investigator-org"
                                        data-validate="isEmpty,isNotNumber,isExist,isNotSpecialChar"
                                        data-err-msg="Provide Valid Input" class="form-control" required />
                                </div>
                                <!-- mobile -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="co-investigator-mob" class="form-label fw-bold"> iv. Mobile </label>
                                    <input type="number" name="co-investigator-mob" id="co-investigator-mob"
                                        data-validate="isEmpty,isNumber" data-err-msg="Provide Valid Input"
                                        class="form-control" required />
                                </div>
                                <!-- email -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="co-investigator-email" class="form-label fw-bold"> v. Email </label>
                                    <input type="text" name="co-investigator-email" id="co-investigator-email"
                                        data-validate="isEmpty,isExist" data-err-msg="Provide Valid Input"
                                        class="form-control" required />
                                </div>
                                <hr>
                                <br>
                                <!-- 5.	Duration of the project months -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="project-duration" class="form-label fw-bold">Duration of the project
                                        months</label>
                                    <input type="date" name="project-duration" id="project-duration"
                                        data-validate="isEmpty,isExist,isDate" data-err-msg="Numeric value is requird."
                                        class="form-control" required>
                                </div>
                                <!-- 6.	Total Cost of the project: Rs. -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="total-cost" class="form-label fw-bold">Total Cost of the project
                                        Rs</label>
                                    <input type="number" name="total-cost" id="total-cost"
                                        data-validate="isEmpty,isNumber" data-err-msg="Numeric value is required."
                                        class="form-control" required>
                                </div>
                                <!-- Consumables Amount -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="consumables-amount" class="form-label fw-bold">Consumables
                                        Amount</label>
                                    <input type="number" name="consumables-amount" id="consumables-amount"
                                        data-validate="isEmpty,isNumber" data-err-msg="Numeric value is required."
                                        class="form-control" required>
                                </div>
                                <!-- Evaluation and analysis  -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="NMIMSAuthors" class="form-label fw-bold">Evaluation and analysis
                                        Amount</label>
                                    <input type="number" name="volumes" id="evaluation-analysis-amount"
                                        data-validate="isEmpty,isNumber" data-err-msg="Numeric value is required."
                                        class="form-control" required>
                                </div>
                                <!-- Any other -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="other-amount" class="form-label fw-bold">Any other</label>
                                    <input type="number" name="other-amount" id="other-amount"
                                        data-validate="isEmpty,isNumber" data-err-msg="Numeric value is required."
                                        class="form-control" required>
                                </div>
                                <!--SECTION II PROJECT DETAILS -->
                                <hr class="bg-dark">
                                <div class="form-group col-lg-12 col-md-6 col-sm-12 mb-4">
                                    <label for="" class="form-label fw-bold modal-title-head">SECTION II PROJECT DETAILS
                                    </label>
                                </div>
                                <hr>
                                <!-- 1.	2.	project title -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="background-field" class="form-label fw-bold">Title </label>
                                    <input type="text" name="project-title" id="project-title"
                                        data-validate="isEmpty,isExist" data-err-msg="Provide Valid Input"
                                        class="form-control" required />
                                </div>
                                <!-- 1.	2.	Background -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="background-field" class="form-label fw-bold">Background </label>
                                    <input type="text" name="project-title" id="project-back-ground"
                                        data-validate="isEmpty,isExist" data-err-msg="Provide Valid Input"
                                        class="form-control" required />
                                </div>
                                <!-- 3.	Review of status of research in the field (National & International): -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="review-status-research-field" class="form-label fw-bold">Review of
                                        status of research in the field (National & International)</label>
                                    <select id="research-status" class="form-control gap-10" required>
                                        <option value="" selected>Select Review Status</option>
                                        <optgroup label="National">
                                            <option value="National - Ongoing">National - Ongoing</option>
                                            <option value="National - Completed">National - Completed</option>
                                            <option value="National - Not Applicable">National - Not Applicable</option>
                                        </optgroup>
                                        <optgroup label="International">
                                            <option value="International - Ongoing">International - Ongoing</option>
                                            <option value="International - Completed">International - Completed</option>
                                            <option value="International - Not Applicable">International - Not
                                                Applicable</option>
                                        </optgroup>
                                    </select>
                                </div>
                                <!-- 4.	Scientific importance of the project-->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="scientific-importance" class="form-label fw-bold">Scientific importance
                                        of the project</label>
                                    <select class="form-control gap-10" id="scientific-importance"
                                        data-validate="isEmpty,isExist"
                                        data-err-msg="This field is required and select value " required>
                                        <option value="" selected>Select option</option>
                                        <option value="High">High - Significant impact and contribution to scientific
                                            knowledge</option>
                                        <option value="Medium">Medium - Moderate impact and contribution</option>
                                        <option value="Low">Low - Limited impact and contribution</option>
                                    </select>
                                </div>

                                <!-- 5.	Hypothesis/Rationale -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="ugc-indexed"
                                        class="form-label fw-bold mb-3">Hypothesis/Rationale</label><br>
                                    <div class="form-check-inline">
                                        <label for="is-hypothesis" class="form-check-label">
                                            <input type="radio" class="form-check-input" id="is-hypothesis"
                                                name="hypothesis" value="Hypothesis"> Hypothesis
                                        </label>
                                    </div>
                                    <div class="form-check-inline">
                                        <label for="is-rationale" class="form-check-label">
                                            <input type="radio" class="form-check-input" id="is-rationale"
                                                name="hypothesis" value="Rationale" checked> Rationale
                                        </label>
                                    </div>
                                </div>
                                <!-- 6.	Objective/s:-->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="objective" class="form-label fw-bold">Objective/s.</label>
                                    <input type="text" name="pages" id="objective" class="form-control"
                                        data-validate="isEmpty,isExist"
                                        data-err-msg="This field is required and select value." required>
                                </div>
                                <!-- 7.	Detailed Methodology -->
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="detailed-methodology" class="form-label fw-bold">Detailed
                                        Methodology</label>
                                    <select id="detailed-methodology" class="form-control gap-10" required>
                                        <option value="" selected>Select Methodology</option>
                                        <option value="Experimental Research">Experimental Research</option>
                                        <option value="Survey Research">Survey Research</option>
                                        <option value="Qualitative Research">Qualitative Research</option>
                                        <option value="Quantitative Research">Quantitative Research</option>
                                        <option value="Case Study">Case Study</option>
                                    </select>

                                </div>
                                <div class="form-group col-lg-4 col-md-6 col-sm-12 mb-4">
                                    <label for="timelines" class="form-label fw-bold">Timelines (Activity Chart
                                        including Milestones)</label>
