    function addInputField(label, className) {
        let inputField = document.createElement('div');
        inputField.classList.add(className);

        let html = `
        <div class="form-group">
            <label for="${label.toLowerCase()}">${label}</label>
            <input type="text" class="form-control" name="${label.toLowerCase()}[]" required>
        </div>
    `;
        inputField.innerHTML = html;

        educationContainer.appendChild(inputField);
    }

    insertDetails.addEventListener('click', function (event) {
        event.preventDefault();
        addInputField('Qualification', 'education-section');
        addInputField('Institution', 'education-section');
        addInputField('Year', 'education-section');
    });

    insertExperience.addEventListener('click', function(event) {
        event.preventDefault();
        addInputField('Possition', 'experience-section');
        addInputField('Organization', 'experience-section');
        addInputField('Period', 'experience-section');
    })

    const educaltionalData = async (url) => {
        let inputFields = document.querySelectorAll('.form-control');

        let currentValues = [];

        inputFields.forEach(field => {
            if (field.value !== '') {
                currentValues.push(field.value);
            }
        });

        educaltionalDetails.push(currentValues);
        payload = {
            educaltionalDetails: educaltionalDetails
        }

        fetchApiFunction(url, payload, educationalIdsContainer);

    }

    function fetchApiFunction(url, payload, arrayIdcontainer){
        fetch(url, {
            method: "POST",
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify(payload)
        })
            .then((Response) => {
                return Response.ok ? Response.json() : 'Network issue'
            })
            .then((data) => {
                console.log('data ====>>>>>', data)
                arrayIdcontainer.push(data.detailsIds)

            })
            .catch((error) => {
                console.log('errrorr ====>>>>>>>', error);
            })
    }

    educationBtn.addEventListener('click', async () => {

        await educaltionalData('<%- BASE_URL %>research/pharmacy-seed-grant-form/investigator-education/insert')
