    function generateTable(header, data, tableContainer) {
        let table = document.createElement('table');
        table.classList.add('table');
        table.border = '1px';

        let thead = document.createElement('thead');
        let headerRow = document.createElement('tr');
        let srNoHeader = document.createElement('th');
        srNoHeader.textContent = 'Sr No';
        headerRow.appendChild(srNoHeader);
        header.forEach(headerItem => {
            let th = document.createElement('th');
            // th.classList.add('col-lg-3');
            th.textContent = headerItem;
            headerRow.appendChild(th);
        });
        thead.appendChild(headerRow);
        table.appendChild(thead);

        let tbody = document.createElement('tbody');
        data.forEach((dataItem, index) => {
            let row = document.createElement('tr');
            let indexCell = document.createElement('td');
            indexCell.textContent = index + 1;
            row.appendChild(indexCell);
            for (let key in dataItem) {
                let td = document.createElement('td');
                td.classList.add('view-content');
                td.textContent = dataItem[key];
                row.appendChild(td);
            }
            tbody.appendChild(row);
        });
        table.appendChild(tbody);

        tableContainer.appendChild(table);
    }



